{
    "jobDefinitionName": "staging-pfb-analysis-run-job",
    "type": "container",
    "parameters": {},
    "containerProperties": {
        "image": "{image}",
        "vcpus": 8,
        "memory": 61000,
        "command": [
            "/pfb/scripts/entrypoint.sh"
        ],
        "jobRoleArn": "",
        "volumes": [{
            "name": "pgdata",
            "host": {
                "sourcePath": "/media/nvme0n1/pgdata"
            }
        }, {
            "name": "tmp",
            "host": {
                "sourcePath": "/media/nvme0n1/tmp"
            }
        }],
        "environment": [{
            "name": "NB_MAX_TRIP_DISTANCE",
            "value": "2680"
        }, {
            "name": "PFB_MAINTENANCE_WORK_MEM",
            "value": "4092MB"
        }, {
            "name": "PFB_MAX_WAL_SIZE",
            "value": "4096MB"
        }, {
            "name": "PFB_SHARED_BUFFERS",
            "value": "7168MB"
        }, {
            "name": "PFB_TEMP_FILE_LIMIT",
            "value": "26214400"
        }, {
            "name": "PFB_WORK_MEM",
            "value": "3072MB"
        }],
        "mountPoints": [{
            "sourceVolume": "pgdata",
            "containerPath": "/pgdata",
            "readOnly": false
        }, {
            "sourceVolume": "tmp",
            "containerPath": "/tmp",
            "readOnly": false
        }],
        "readonlyRootFilesystem": false,
        "privileged": true,
        "ulimits": [],
        "user": ""
    }
}
